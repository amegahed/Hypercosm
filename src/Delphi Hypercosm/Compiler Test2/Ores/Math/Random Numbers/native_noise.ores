//
// scalar noise functions
//


native scalar question noise1
    scalar x;
end;    // noise1
native scalar question noise2
    scalar x, y;
end;    // noise2
native scalar question noise
    vector v;
end;    // noise


//
// vector noise functions
//


native vector question vnoise1
    scalar x;
end;    // vnoise1
native vector question vnoise2
    scalar x, y;
end;    // vnoise2
native vector question vnoise
    vector v;
end;    // vnoise
