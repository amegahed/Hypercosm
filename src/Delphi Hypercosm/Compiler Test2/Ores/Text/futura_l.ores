shape futura_l_word
	string type str;
with
	scalar size = .75;
is
	shape word is
		move to <(-num str / 2 * .125) 0 0>;
		for integer counter = min str .. max str do
			futura_l_letter str[counter];
			move to <.125 0 0>;
		end;
	end;	// word

	word with
		magnify by size;
	end;
end;	// futura_l_word
